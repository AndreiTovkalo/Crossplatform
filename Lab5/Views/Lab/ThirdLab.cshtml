@model string

@{
    ViewData["Title"] = "Lab3";
}

<h1>Lab3</h1>




<form method="post" >
    <div>
        <label for="input">Input path</label>
        <input type="text" name="inputPath" id="input" />
    </div>
    <div>
        <label for="output">Output path</label>
        <input type="text" name="outputPath" id="output" />
    </div>
    <button type="submit">Submit</button>
</form>




@if (!string.IsNullOrEmpty(Model))
{
    <h2>Response:</h2>
    <p>@Model</p>
}